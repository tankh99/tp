@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":PatientListParser" as PatientListParser LOGIC_COLOR
participant "h:HelpCommand" as HelpCommand LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
end box

[-> LogicManager: execute("help")
activate LogicManager
LogicManager -> PatientListParser: parse("help")
activate PatientListParser
create HelpCommand
PatientListParser -> HelpCommand: new HelpCommand()
activate HelpCommand
HelpCommand --> PatientListParser: h
deactivate HelpCommand
LogicManager <-- PatientListParser: h
deactivate PatientListParser
LogicManager -> HelpCommand: execute()
activate HelpCommand
create CommandResult
HelpCommand -> CommandResult: new CommandResult()
activate CommandResult
CommandResult --> HelpCommand
deactivate CommandResult
HelpCommand --> LogicManager: r
deactivate HelpCommand
destroy HelpCommand

[<- LogicManager: Execute Command Successfully
deactivate LogicManager
@enduml
